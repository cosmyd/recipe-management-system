<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jfxbase.recipesystem.controllers.LogInController">
   <children>
      <PasswordField fx:id="passwordField" layoutX="226.0" layoutY="200.0" text="password" />
      <TextField fx:id="usernameField" layoutX="226.0" layoutY="159.0" promptText="username" />
      <Button fx:id="logInButton" layoutX="275.0" layoutY="255.0" mnemonicParsing="false" onAction="#onLogInButtonClick" text="Log In" />
      <Button fx:id="signUpButton" layoutX="503.0" layoutY="347.0" mnemonicParsing="false" onAction="#onSignUpButtonClick" text="Sign Up" />
   </children>
</AnchorPane>
